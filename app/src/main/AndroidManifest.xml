<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.workoutkeeper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".KoreanFish"></activity>
        <activity
            android:name=".TimerAddActivity"
            android:theme="@style/activity_styles"></activity>
        <activity
            android:name=".TimerActivity"
            android:label="@string/toolbar_timer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RMShowActivity"
            android:theme="@style/activity_styles" />
        <activity
            android:name=".RMCalculatorActivity"
            android:theme="@style/activity_styles" />
        <activity
            android:name=".AddTrainingActivity"
            android:label="@string/toolbar_add_your_own_training" />
        <activity
            android:name=".VideoPLayer"
            android:label="@string/title_activity_video_player"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DailyScheduleActivity"
            android:label="Daily Schedule" />
        <activity
            android:name=".ProgressActivity"
            android:configChanges="orientation|screenSize"
            android:label="@string/toolbar_progress">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            </intent-filter>
        </activity>
        <activity
            android:name=".SetsAndRepsActivity"
            android:label=""
            android:theme="@style/sets_and_reps_dialog">

            <!--
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.workoutkeeper.ChooseBodyPartActivity" />
            -->
        </activity>
        <activity
            android:name=".TrainingActivity"
            android:label="@string/title_activity_training"
            android:theme="@style/AppTheme.NoActionBar">

            <!--
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.workoutkeeper.ChooseBodyPartActivity" />
            -->
        </activity>
        <activity
            android:name=".ChooseBodyPartActivity"
            android:label="@string/toolbar_bodypart" />

        <activity
            android:name=".MainActivity"
            android:label="@string/toolbar_fitness">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>